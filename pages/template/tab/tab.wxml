<!--pages/template/tab/tab.wxml-->
 <template name='tab'>
	<view class='order_list'>
	<view class='tabs'>
		 <view class='tab' wx:for="{{tabs}}" wx:for-item="tab" data-index='{{index}}' bindtap='_switch'>
			<text class='{{crrIndex == index ? "tab_crr" : ""}}'>{{tab}}</text>
		</view> 
		<!-- <text>{{tabs}}</text> -->
	</view>
	</view>
</template> 

