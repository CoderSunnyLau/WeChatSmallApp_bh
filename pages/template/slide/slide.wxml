<msg id='msg'></msg>
<test bind:search='searchPro' bind:scan='scanPro' id='bar'></test>

<view class='pageTab' id='tab'>
  <view class="tabItem {{isSelect==index?'curr':''}}" wx:for='{{tabArr}}' bindtap='tabSelect' data-tIdx='{{index}}'>{{item}}</view>
	<view class='tabItem' bindtap='screen'>筛选</view>
</view>
<block wx:for='{{arr}}'>
  <view style='height: {{bgHeight}}px;background-color:#fff;'>
    <view>{{item.bb}}</view>
    <block wx:for='{{mArr}}' wx:for-item='mItem'>
      <view>{{item.aa}}</view>
      <view>{{mItem.cc}}</view>
    </block>
  </view>
</block>