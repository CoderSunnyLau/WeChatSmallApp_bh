<msg></msg>
<block wx:for='{{arr}}'>
  <view>
    <view>{{item.bb}}</view>
    <block wx:for='{{mArr}}' wx:for-item='mItem'>
      <view>{{item.aa}}</view>
      <view>{{mItem.cc}}</view>
    </block>
  </view>
</block>
<view class='product-item'>
  <form class='buy-action' bindsubmit='addToShopcart'>
    <view class='shopcart' form-type='submit'></view>
    <view class='action-box'>
      <text class='reduce'>-</text>
      <input type='number' bindinput='amountAdd' name='amount'/>
      <text class='add'>+</text>
    </view>
  </form>
</view>